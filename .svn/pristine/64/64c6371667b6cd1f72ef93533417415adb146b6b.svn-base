$def with (bot_id,data)
<div class="pageContent j-resizeGrid">
	<div class="panelBar">
		<ul class="toolBar">
			<li><a href="/webui/get_group_list/?bot_id=$bot_id" target="navTab" rel="get_group_list" class="icon" title="群列表"><span>刷新</span></a></li>
		</ul>
	</div>
	<table class="table" width="100%" layoutH="100">
		<thead>
			<tr>
				<th>NickName</th>
				<th>UserName</th>
				<th>MemberNums</th>
				<th width="70">操作</th>
			</tr>
		</thead>
		<tbody>
		$for temp in data['data']['group_list']:
			<tr>
				<td>$temp['NickName']</td>
				<td>$temp['UserName']</td>
				<td>
				    <a title="群用户列表" target="navTab" rel="get_group_member_list" href="/webui/get_group_member_list/?bot_id=$bot_id&gid=$temp['UserName']" >$temp['MemberNums']</a>
				</td>
				<td></td>
			</tr>
		</tbody>
    </table>
    <div class="panelBar" >
    	
    </div>
</div>
